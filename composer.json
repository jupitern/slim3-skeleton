{
  "name": "jupitern/slim3-skeleton",
  "description": "Slim starter / Slim skeleton package to boost your development with Slim framework",
  "keywords": [
    "slim", "slim php", "Slim skeleton", "Slim starter", "skeleton", "starter", "php framework", "api", "cli"
  ],
  "license": "MIT",
  "require": {
    "php": ">= 7.0",
    "slim/slim": "3.12.*",
    "monolog/monolog": "1.24.*",
    "slashtrace/slashtrace": "1.1.*",
    "league/flysystem": "1.0.*",
    "league/plates": "3.3.*",
    "illuminate/database": "5.7.*",
    "phpmailer/phpmailer": "6.0.*",
    "naroga/redis-cache": "1.1.*",
    "hellogerard/jobby": "3.4.*",
    "league/oauth2-server": "^7.4"
  },
  "require-dev": {
  },
  "autoload": {
      "psr-4": {
          "App\\": "app/",
          "Lib\\": "lib/"
      },
      "files": ["lib/helpers.php"]
  },
  "suggest": {
  	"respect/validation": "1.1.*",
    "jupitern/table": "1.*",
    "slim/twig-view": "2.4.*"
  }
}
